<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!--    we need to suppress modules by reason of 3DS domain specific -->
    <suppress checks="LocalVariableName"
              files="com[\\/]rbkmoney[\\/]threeds[\\/].*.java"/>
    <suppress checks="ParameterName"
              files="com[\\/]rbkmoney[\\/]threeds[\\/].*.java"/>
    <suppress checks="MethodName"
              files="com[\\/]rbkmoney[\\/]threeds[\\/].*.java"/>
    <suppress checks="AbbreviationAsWordInName"
              files="com[\\/]rbkmoney[\\/]threeds[\\/].*.java"/>
    <suppress checks="LineLength"
              files="StorageServiceConfig.java|RBKMoneyThreeDsMethodTest.java"/>
</suppressions>
